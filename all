{
    int n, i;
    float num[100], sum=0.0, average;
    cout << "Enter the size of array: ";
    cin >> n;
    for(i = 0; i < n; ++i)
    {
        cout << i + 1 << ". Enter number: ";
        cin >> num[i];
        sum += num[i];
    }
    average = sum / n;
